{
  "name": "octofarm-client",
  "version": "1.7.4",
  "description": "The website client for OctoFarm bundled using webpack.",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OctoFarm/OctoFarm"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "octofarm": "./index.js"
  },
  "keywords": [
    "octofarm",
    "frontend",
    "ui",
    "website",
    "javascript",
    "3d-printing"
  ],
  "author": "James Mackay (NotExpectedYet)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "apexcharts": "^3.41.0",
    "axios": "^1.4.0",
    "babel-loader": "^9.1.2",
    "before-build-webpack": "^0.2.13",
    "bootbox": "^5.6.0",
    "bootstrap": "^5.3.2",
    "browserlist": "^4.21.7",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.2",
    "flatted": "^3.2.7",
    "gridstack": "^8.4.1",
    "jquery": "^3.7.1",
    "litepicker": "^2.0.15",
    "lodash": "^4.17.23",
    "mini-css-extract-plugin": "^2.7.6",
    "noty": "^3.2.0-beta-deprecated",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "source-map-loader": "^5.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engine-strict": true,
  "engines": {
    "npm": ">= 6.10.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "core-js": "^3.31.0"
  }
}
