{
  "name": "octofarm-server",
  "version": "1.7.4",
  "main": "app.js",
  "description": "OctoFarm is a easy to setup and install web interface that unifies your Octoprint instances for easy monitoring of all your printers. OctoFarm allows you to connect to multiple octoprint instances on your network allowing creating a farm management system. You can control, manage, upload files to any of your instances without ever leaving the tab it's open in.",
  "scripts": {
    "prestart": "npm ci",
    "start": "pm2 flush && pm2 start ecosystem.config.js",
    "dev": "nodemon ./app.js",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "prettier": "prettier --write \"./**/*.js\" \"../client/**/*.js\"\"",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OctoFarm/OctoFarm"
  },
  "keywords": [
    "octofarm",
    "octo",
    "octoprint",
    "pi",
    "node"
  ],
  "author": "James Mackay (NotExpectedYet)",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.3.1",
    "auto-git-update": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "dotnet": "^1.1.4",
    "ejs": "^3.1.9",
    "express": "^4.21.1",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.17.4",
    "flatted": "^3.2.7",
    "helmet": "^7.0.0",
    "influx": "^5.9.4",
    "is-docker": "^2.3.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.23",
    "lookpath": "^1.2.2",
    "luxon": "^3.4.0",
    "marked": "^5.1.0",
    "migrate-mongo": "^10.0.0",
    "mjpeg-camera": "^2.0.1",
    "mjpeg-consumer": "^2.0.0",
    "mjpeg-decoder": "^1.0.3",
    "mongoose": "^7.7.0",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.12",
    "node-input-validator": "^5.0.1",
    "node-upnp-ssdp": "^0.1.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.2",
    "primus": "^8.1.0",
    "pws": "^7.0.1",
    "request": "^2.88.2",
    "sanitize": "^2.1.2",
    "semver": "^7.5.4",
    "sharp": "^0.32.1",
    "simple-git": "^3.19.0",
    "systeminformation": "^5.20.0",
    "table": "^6.9.0",
    "toad-scheduler": "^1.7.0",
    "wake_on_lan": "^1.0.0",
    "winston": "^3.10.0",
    "ws": "^8.14.1",
    "xml2js": "^0.4.23"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.10.0"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "views/assets/*",
      "logs/*",
      "client_src/*"
    ],
    "delay": 2500
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.10"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "release-it": "^16.0.0"
  }
}
